    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.3",
    "create-svg-sprite": "^1.0.4",
    "eslint": "^5.16.0",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint": "^2.5.0",
