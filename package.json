{
  "name": "herpproject.gitlab.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "svgo": "svgo --config='{ \"plugins\": [ { \"removeAttrs\": { \"attrs\": \"(fill|stroke)\" } } ] }' ./assets",
    "sprite": "create-svg-sprite --input ./assets --output ./compiled",
    "dev": "npm run sprite & npx pug src --out compiled --pretty --watch & npx webpack-dev-server --mode development --watch --hot",
    "pug": "npx pug src --out compiled --pretty",
    "pack": "webpack --mode production",
    "compile": "npm run svgo && npm run sprite && npm run pug && npm run pack"
  },
  "homepage": "https://gitlab.com/herpproject/herpproject.gitlab.io#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/herpproject/herpproject.gitlab.io.git"
  },
